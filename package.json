{
    "author": "Christoph Guttandin",
    "bugs": {
        "url": "https://github.com/chrisguttandin/midi-json-parser/issues"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@babel/runtime": "^7.28.6",
        "midi-json-parser-broker": "^4.0.130",
        "midi-json-parser-worker": "^8.1.64",
        "tslib": "^2.8.1"
    },
    "description": "This module is parsing midi files into a human-readable JSON object.",
    "devDependencies": {
        "@babel/core": "^7.28.6",
        "@babel/plugin-external-helpers": "^7.27.1",
        "@babel/plugin-transform-runtime": "^7.28.5",
        "@babel/preset-env": "^7.28.6",
        "@commitlint/cli": "^20.3.1",
        "@commitlint/config-angular": "^20.3.1",
        "@rollup/plugin-babel": "^6.1.0",
        "@rollup/plugin-replace": "^6.0.3",
        "@vitest/browser-webdriverio": "^4.0.17",
        "babel-loader": "^10.0.0",
        "commitizen": "^4.3.1",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.57.0",
        "eslint-config-holy-grail": "^61.0.6",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "memfs": "^4.56.9",
        "prettier": "^3.8.1",
        "rimraf": "^6.1.2",
        "rollup": "^4.55.1",
        "terser-webpack-plugin": "^5.3.16",
        "tsconfig-holy-grail": "^15.0.3",
        "tslint": "^6.1.3",
        "tslint-config-holy-grail": "^56.0.7",
        "typescript": "^5.9.3",
        "vitest": "^4.0.17",
        "webpack": "^5.104.1",
        "webpack-cli": "^6.0.1"
    },
    "files": [
        "build/es2019/",
        "build/es5/",
        "src/"
    ],
    "homepage": "https://github.com/chrisguttandin/midi-json-parser",
    "keywords": [
        "MIDI"
    ],
    "license": "MIT",
    "main": "build/es5/bundle.js",
    "module": "build/es2019/module.js",
    "name": "midi-json-parser",
    "repository": {
        "type": "git",
        "url": "https://github.com/chrisguttandin/midi-json-parser.git"
    },
    "scripts": {
        "build": "rimraf build/* && webpack --config config/webpack/worker-es2019.js && tsc --project src/tsconfig.json && rollup --config config/rollup/bundle.mjs",
        "lint": "npm run lint:config && npm run lint:src && npm run lint:test",
        "lint:config": "eslint --config config/eslint/config.json --ext .cjs --ext .js --ext .mjs --report-unused-disable-directives config/",
        "lint:src": "tslint --config config/tslint/src.json --project src/tsconfig.json src/*.ts src/**/*.ts",
        "lint:test": "eslint --config config/eslint/test.json --ext .js --report-unused-disable-directives test/",
        "prepare": "husky",
        "prepublishOnly": "npm run build",
        "test": "npm run lint && npm run build && npm run test:unit",
        "test:unit": "if [ \"$TYPE\" = \"\" -o \"$TYPE\" = \"unit\" ]; then npx vitest --config config/vitest/unit.ts; fi"
    },
    "types": "build/es2019/module.d.ts",
    "version": "8.1.71"
}
